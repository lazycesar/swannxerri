<form method="post" action="{{ path('admin_login_delete', {'id': admin_login.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin_login.id) }}">
    <button class="btn">Delete</button>
</form>
