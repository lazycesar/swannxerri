{% extends 'base.html.twig' %}

{% block title %}AdminLogin{% endblock %}

{% block body %}
    <h1>AdminLogin</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin_login.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ admin_login.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ admin_login.password }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ admin_login.level }}</td>
            </tr>
            <tr>
                <th>Creation_date</th>
                <td>{{ admin_login.creationDate ? admin_login.creationDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_login_index') }}">back to list</a>

    <a href="{{ path('admin_login_edit', {'id': admin_login.id}) }}">edit</a>

    {{ include('admin_login/_delete_form.html.twig') }}
{% endblock %}
